{% block javascripts %}
    {{ parent() }}
    <script src="{{asset('packs/js/galleryCarousel.js')}}"></script>
    <script>
        const Carousel = new IncarnateGalleryCarousel('artwork','piece-of-art');
        function carouselDeploy(ev){
            IncarnateGalleryCarousel.deploy(ev,Carousel.carousel,'piece-of-art');
        }
        Carousel.items.forEach(element=>{
            element.addEventListener('click',carouselDeploy);
        });
    </script>
{% endblock %}

