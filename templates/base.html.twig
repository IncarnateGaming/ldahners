        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="d-flex flex-column">
        <div class="inc-nav-body inc-text-ldahners4 inc-bg-ldahners2-dark">
            <div class="inc-nav-flex-holder"></div>
            <div class="inc-body d-flex flex-column">
                <div class="inc-flex-grow">
                {% for message in app.flashes('success') %}
                    <div class="alert alert-success">
                        {{ message }}
                    </div>
                {% endfor %}
                {% for message in app.flashes('failure') %}
                    <div class="alert alert-danger">
                        {{ message }}
                    </div>
                {% endfor %}
                {% block body_indentation %}
                    <div class="inc-indentation mt-2">
                        {% block body %}{% endblock %}
                    </div>
                {% endblock %}
                </div>
                <footer class="page-footer font-small inc-banners inc-bg-ldahners4-dark">
                    <div class="footer-copyright text-center py-3 inc-text-light">
                        Â© 2020 <a class="inc-text-light" href="https://www.incarnategamingllc.com/hire/">Incarnate Gaming LLC</a>
                    </div>
                </footer>
            </div>
        </div>
        <nav class="navbar inc-sticky-header inc-banners align-content-start inc-bg-ldahners4-dark" id="navbar">
            <button class="navbar-toggler text-dark mr-auto inc-button-color inc-bg-ldahners7" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <a id="incarnate-logo" class="navbar-brand inc-text-light ml-2" href="/">Laury Dahners<img src="{{ asset('/assets/images/0-simple-donsaii-tiny.jpg') }}" class="ml-3 mr-1"></a>
            <div class="collapse navbar-collapse overflow-auto" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_home')|index_stripper }}'">Home</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_stories')|index_stripper }}'">Novels & Short Stories</button>
                    </li>
                    {%  if is_granted('ROLE_ADMIN') %}
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_new_series')|index_stripper }}'">New Series</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_new_book')|index_stripper }}'">New Book</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_new_merchandise')|index_stripper }}'">New Merchandise</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_new_review')|index_stripper }}'">New Review</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_list_series')|index_stripper }}'">List Series</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_list_books')|index_stripper }}'">List Books</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_list_merchandise')|index_stripper }}'">List Merchandise</button>
                        </li>
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_admin_list_review')|index_stripper }}'">List Review</button>
                        </li>
                    {% endif %}
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_songs_vocal')|index_stripper }}'">Songs</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_songs_instrumental')|index_stripper }}'">Instrumental Music</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_art_digital')|index_stripper }}'">Digital Art</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_art_physical')|index_stripper }}'">Art</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_charity')|index_stripper }}'">Charity Surgery</button>
                    </li>
                    <li class="nav-item">
                        <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_professional')|index_stripper }}'">Orthopedic</button>
                    </li>
                    {#                    <li class="nav-item">#}
                    {#                        <button class="btn btn-primary m-2" id="contentDropDown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Content</button>#}
                    {#                        <div class="dropdown-menu overflow-auto" aria-labelledby="contentDropdown">#}
                    {#                            <a class="dropdown-item text-primary" href="{{ path('inc_backgrounds')|index_stripper }}">Backgrounds</a>#}
                    {#                        </div>#}
                    {#                    </li>#}
                    <button class="btn m-2 inc-button-color inc-bg-ldahners7" id="topButton" type="button" aria-label="Navigate to Top">Top</button>
                    {% block backButton %}{% endblock %}
                </ul>
            </div>
            {% block additionalLogos %}
                <img src="{{ asset('/assets/images/0-simple-help-tiny.jpg') }}" class="mr-1 inc-closing-logo">
            {% endblock %}
            {%  if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                <list>
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <button class="btn m-2 inc-button-color inc-bg-ldahners7" onclick="window.location.href='{{ path('app_account_changePassword')|index_stripper }}'">Change Password</button>
                        </li>
                    </ul>
                </list>
            {% endif %}
        </nav>
        {% block javascripts %}
            <script>
                var IncarnateAnalyticsTag = 'UA-155417623-1';
            </script>
            <script src="{{asset('packs/js/navFunctions.js')}}"></script>
